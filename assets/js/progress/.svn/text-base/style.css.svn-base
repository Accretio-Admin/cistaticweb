
.progress-wrapper {
	width: auto;
	margin: 30px auto 0;
}
.html5-progress-bar {
	padding: 5px 5px;
	border-radius: 3px;
	background-color: #fff;
	/*box-shadow:  0px 1px 2px 0px rgba(0, 0, 0, .2);*/
}
.html5-progress-bar progress {
	background-color: #f6f6f6;
	border: 0;
	width: 100%;
	height: 18px;
	border-radius: 3px;
}
.html5-progress-bar progress::-webkit-progress-bar {
	background-color: #f6f6f6;
	border-radius: 3px;
}
.html5-progress-bar progress::-moz-progress-bar { background: #0063a6; }
.html5-progress-bar progress::-webkit-progress-value {
	background: #fbd504;
	background: -moz-linear-gradient(top,  #fbd504 0%, #fbb804 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#fbd504), color-stop(100%,#fbb804));
	background: -webkit-linear-gradient(top,  #fbd504 0%,#fbb804 100%);
	background: -o-linear-gradient(top,  #fbd504 0%,#fbb804 100%);
	background: -ms-linear-gradient(top,  #fbd504 0%,#fbb804 100%);
	background: linear-gradient(to bottom,  #fbd504 0%,#fbb804 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#fbd504', endColorstr='#fbb804',GradientType=0 );
	border-radius: 3px;
}
.html5-progress-bar progress::-moz-progress-bar {
	background: #fbd504;
	background: -moz-linear-gradient(top,  #fbd504 0%, #fbb804 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#fbd504), color-stop(100%,#fbb804));
	background: -webkit-linear-gradient(top,  #fbd504 0%,#fbb804 100%);
	background: -o-linear-gradient(top,  #fbd504 0%,#fbb804 100%);
	background: -ms-linear-gradient(top,  #fbd504 0%,#fbb804 100%);
	background: linear-gradient(to bottom,  #fbd504 0%,#fbb804 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#fbd504', endColorstr='#fbb804',GradientType=0 );
	border-radius: 3px;
}
.html5-progress-bar .progress-value {
	padding: 0px 5px;
	line-height: 20px;
	margin-left: 5px;
	font-size: .8em;
	color: #555;
	height: 18px;
}